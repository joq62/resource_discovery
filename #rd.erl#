test_phoscon()->
    io:format("Start ~p~n",[{?MODULE,?FUNCTION_NAME}]),
 
    glurk= rd:call(phoscon_control,get_maps,[],5000),
    pong=rd:call(phoscon_control,ping,[],5000),
    
    ok.